:80 {

    # handle /socket.io/* {
    #     reverse_proxy http://autocad-to-gis-websocket:5000
    # }

    handle /websockets/* {
        reverse_proxy http://autocad-to-gis-websocket:5000
    }    

    handle /api/autocad-to-gis/convert/* {
        reverse_proxy http://autocad-to-gis:5000
    }    

    handle /api/* {
        reverse_proxy http://autocad-to-gis:5000
    }

    handle_path /data/* {
        root * /srv/data/
        file_server browse
    }

    handle * {
        root * /srv    
        file_server browse
    }
}

